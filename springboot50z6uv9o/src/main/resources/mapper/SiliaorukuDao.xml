	<select id="selectListVO"
		resultType="com.entity.vo.SiliaorukuVO" >
		SELECT * FROM siliaoruku  siliaoruku         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.SiliaorukuVO" >
		SELECT  siliaoruku.* FROM siliaoruku  siliaoruku 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.SiliaorukuView" >

		SELECT  siliaoruku.* FROM siliaoruku  siliaoruku 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.SiliaorukuView" >
		SELECT * FROM siliaoruku  siliaoruku <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
