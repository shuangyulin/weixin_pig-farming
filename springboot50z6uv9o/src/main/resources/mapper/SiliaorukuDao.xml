<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.SiliaorukuDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.SiliaorukuEntity" id="siliaorukuMap">
        <result property="rukubianhao" column="rukubianhao"/>
        <result property="siliaobianhao" column="siliaobianhao"/>
        <result property="siliaomingcheng" column="siliaomingcheng"/>
        <result property="siliaoguige" column="siliaoguige"/>
        <result property="siliaojiage" column="siliaojiage"/>
        <result property="shuliang" column="shuliang"/>
        <result property="gongyingshang" column="gongyingshang"/>
        <result property="caigouhetong" column="caigouhetong"/>
